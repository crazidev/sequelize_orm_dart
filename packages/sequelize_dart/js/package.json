{
  "name": "sequelize_dart_bridge",
  "version": "1.0.0",
  "description": "Bridge server for Sequelize Dart",
  "main": "bridge_server.bundle.js",
  "scripts": {
    "build": "npm run build:stdio && npm run build:worker",
    "build:stdio": "esbuild src/bridge_server.ts --bundle --platform=node --target=node18 --outfile=bridge_server.bundle.js --minify --external:fsevents --external:@sequelize/mssql --external:@sequelize/sqlite3 --external:@sequelize/db2-ibmi --external:@sequelize/db2 --external:@sequelize/snowflake --format=cjs --banner:js=\"#!/usr/bin/env node\"",
    "build:worker": "esbuild src/bridge_server_worker.ts --bundle --platform=node --target=node18 --outfile=bridge_server_worker.bundle.js --minify --external:fsevents --external:@sequelize/mssql --external:@sequelize/sqlite3 --external:@sequelize/db2-ibmi --external:@sequelize/db2 --external:@sequelize/snowflake --format=cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.47",
    "@sequelize/postgres": "^7.0.0-alpha.47",
    "@sequelize/mysql": "^7.0.0-alpha.47",
    "@sequelize/mariadb": "^7.0.0-alpha.47",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6"
  }
}
