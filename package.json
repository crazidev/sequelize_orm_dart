{
  "name": "my_dart_server",
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md}\" && dart format .",
    "format:check": "prettier --check \"**/*.{js,json,md}\" && dart format --set-exit-if-changed .",
    "lint": "dart analyze .",
    "lint:fix": "dart fix --apply ."
  },
  "devDependencies": {
    "prettier": "^3.0.0"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.48",
    "@sequelize/mariadb": "^7.0.0-alpha.48",
    "@sequelize/mysql": "^7.0.0-alpha.48",
    "@sequelize/postgres": "^7.0.0-alpha.48",
    "@sequelize/sqlite3": "^7.0.0-alpha.48"
  }
}
